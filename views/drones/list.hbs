<h2>Available drones</h2>
<div id="drone-list-container">     
    {{#each returnedDrones}}
        <div id="drone-card">
            <h4>{{name}}</h4>
            <p>Propellers: {{propellers}}</p>
            <p>Maximum speed: {{maxSpeed}}</p>
            <a href="/drones/{{_id}}/edit">Update drone</a>
            <form method="POST" action="/drones/{{_id}}/delete">
                <button>Delete drone</button>
            </form>
        </div>
    {{/each}}
        
</div>